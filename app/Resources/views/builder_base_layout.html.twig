<!DOCTYPE html>
<html lang="fr-fr">

<head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-149535256-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());

        gtag('config', 'UA-149535256-1');
    </script>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{% block title %}Accent'Tonique, École de langues, anglais et espagnol, à Toulouse{% endblock %}</title>
    <meta name="description" content={% block seoDescription %}
            "École de langues, anglais et espagnol, à Toulouse, tous niveaux; Nous proposons des cours en groupe, en individuel, des formations professionnelles, des stages et voyages linguistiques. Accent'Tonique"
        {% endblock %}/>
    <meta name="keywords" content={% block seoKeywords %}
            "école de langue, anglais, espagnol, cours toulouse, accent tonique, tous niveaux"
        {% endblock %} />

    <link rel="icon" type="image/x-icon" href="{{ asset('assets/logo-AccentTonique-2018-64x64.ico') }}" /> 
    
    {% block stylesheets %} 
    {# On charge le CSS de bootstrap depuis le site directement #}
    

    <meta name="author" content="Funga.fr">
    <meta name="copyright" content="2022 © ACCENT TONIQUE">
    <meta http-equiv="content-type" content="text/html;UTF-8">
    <meta http-equiv="content-language" content="fr">

    <meta property="og:type" content="website" />
    <meta property="og:title" content="Accent Tonique" />
    <meta property="og:description" content={{ block('seoDescription') }} />
    <meta property="og:url" content="https://www.accent-tonique-31.com/" />
    <meta property="og:site_name" content="Accent Tonique" />
    <meta property="og:locale" content="fr_FR" />
    <meta property="og:image" content="https://www.accent-tonique-31.com/assets/logo_AccentTonique_2023_3.png" />
    <link rel="alternate" href="https://www.accent-tonique-31.com/" hreflang="fr-fr" />
    <link rel="alternate" href="https://www.accent-tonique-31.com/" hreflang="x-default" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" />
    <!-- Optional theme -->
    <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous"> -->

    {# select2_bootstrap #}
    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/css/select2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="{{ asset('assets/css/select2/select2_bootstrap.css') }}" /> 
    {# css SonataUserBundle #} 
    {% stylesheets '@ApplicationSonataUserBundle/Resources/public/css/main.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" /> {% endstylesheets %} 
    {# css buidler #} {% stylesheets '@AdminBundle/Resources/public/css/main.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" /> {% endstylesheets %}

    <script src="https://kit.fontawesome.com/e00cbb0e10.js" crossorigin="anonymous"></script>
    {# <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp"
        crossorigin="anonymous">  #}
    {% endblock %} 
    
    <!-- Global site tag (gtag.js) - Google Analytics PROD ONLY-->
    {# <script async src="https://www.googletagmanager.com/gtag/js?id=UA-122394378-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());

        gtag('config', 'UA-122394378-1');
    </script> #}

    
</head>

<body class=""> {# body-back #}
     <div class="mx-auto col-sm-11 col-xl-9"> {# container #}
        <div class="page px-3">
            <div class="row m-0">
                <div class="col-sm-2 p-0 align-self-center">
                    <a href="/accueil/"><img class="logo_banner" alt="logo Accent Tonique" src="/assets/logo_AccentTonique_2023_3.png"/></a>
                </div>
			    <div class="col-sm-10 p-0">
                    <img alt="bannière principale d'Accent Tonique" class="icone_banner d-flex ml-auto" src="/assets/headerAT2.jpeg" />
                </div>
            </div>
            {% block menu %}
            <h3>Pages:</h3>
            <ul class="nav nav-pills nav-stacked">
                <li>
                    <a href="{{ path('homepage') }}">Accueil</a>
                </li>
            </ul>
            {% endblock %}
            {% block fos_user_content %}
            {% endblock %}

            <div class="row m-0">
                <div class="body col-lg-9">
                    <div class="row justify-content-md-center m-0">
                    {% block body %}{% endblock %}
                    </div>
                    
                </div>
                <div class="lateral col-lg-3">
                    {% block lateral %} {% include "lateral.html.twig"%} {% endblock %}
                </div>
            </div>

        </div>
        {% block footer %}
            <div class="Footer">
                <p>Site créé par <a href="https://www.funga.fr" >Funga</a></p>
            </div>
        {% endblock %}
    </div>

    
</body>


{% block javascripts %}
    {# jquery #}
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
        crossorigin="anonymous"></script>
    {# jquery UI #}
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>

    {# bootstrap #}
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>

    {# select2 #}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/skrollr/0.6.30/skrollr.min.js"></script>

    {% javascripts '@BuilderPageBundle/Resources/public/js/carousel.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% endblock %}
</html>